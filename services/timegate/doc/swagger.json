{
  "openapi": "3.0.0",
  "info": {
    "version": "0.0.1",
    "title": "Multiplayer API service"
  },
  "servers": [
    {
      "url": "/v0/api"
    }
  ],
  "tags": [
    {
      "name": "Health",
      "description": "Health"
    },
    {
      "name": "Healthz",
      "description": "Healthz"
    },
    {
      "name": "Dialogue hub"
    },
    {
      "name": "Epoch engine"
    },
    {
      "name": "Minds of time"
    },
    {
      "name": "Vault of time"
    }
  ],
  "paths": {
    "/health": {
      "get": {
        "summary": "Get health.",
        "description": "Get health.",
        "tags": [
          "Health"
        ],
        "operationId": "geHealth",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Health.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Health"
                }
              }
            }
          }
        }
      }
    },
    "/healthz": {
      "get": {
        "summary": "Get healthz.",
        "description": "Get healthz.",
        "tags": [
          "Healthz"
        ],
        "operationId": "geHealthz",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Healthz.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Healthz"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "CookieAuth": {
        "type": "apiKey",
        "in": "cookie",
        "name": "Multiplayer_SID"
      }
    },
    "schemas": {
      "Health": {
        "type": "object",
        "properties": {
          "messageQueue": {
            "type": "boolean",
            "readOnly": true
          },
          "mongodb": {
            "type": "boolean",
            "readOnly": true
          }
        }
      },
      "Healthz": {
        "type": "string"
      },
      "Error": {
        "type": "object",
        "required": [
          "code",
          "message",
          "status"
        ],
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          },
          "status": {
            "type": "string"
          }
        }
      }
    }
  }
}
